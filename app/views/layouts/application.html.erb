<!DOCTYPE html>
<html>
  <head>
    <title>FUSION</title>
    <%= stylesheet_link_tag "reset", "text", "960", "jquery-ui", 'imgareaselect-animated', "colorpicker", "application", :cache => true %>
    <%= yield(:before_head) %>
    <%= javascript_include_tag 'jquery-1.4.4.min', 'jquery-ui-1.8.6.min', 'jquery-imgareaselect-0.9.3.min', 'base64', 'canvas2image', "colorpicker", 'whiteboard', 'whiteboardui.jquery','rails', 'application', :cache => true %>
    <%= yield(:head) %>
    <%= favicon_link_tag %>
    <%= csrf_meta_tag %>
  </head>
  <body>
    <div class="container_12">
      <div id="header" class="grid_12">
        Welcome <%= current_user.name %>, <%= link_to "Logout", logout_path %>
      </div>

      <%= yield %>

      <div id="footer" class="grid_12">
        Footer
      </div>
    </div>
  </body>
</html>
